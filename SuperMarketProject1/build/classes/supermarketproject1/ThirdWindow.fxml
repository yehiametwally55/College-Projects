            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="cash" layoutX="14.0" layoutY="233.0" mnemonicParsing="false" onAction="#CashPayHandler" prefHeight="30.0" prefWidth="116.0" text="cash">
