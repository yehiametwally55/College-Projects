            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="visa" layoutX="14.0" layoutY="158.0" mnemonicParsing="false" onAction="#VisaPayHandler" prefHeight="26.0" prefWidth="116.0" text="visa">
